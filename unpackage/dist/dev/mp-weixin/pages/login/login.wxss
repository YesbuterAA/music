.login_bar {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
  padding-right: 126rpx;
  margin-top: 20rpx;
}
.login_bar button {
  margin: 0;
  padding: 0;
  margin-left: 30rpx;
  width: 150rpx;
}
.placeholder {
  color: #888;
  margin-left: 15rpx;
  font-size: 30rpx;
  font-weight: 800;
}
.login_from {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-top: 20rpx;
}
.login_from input {
  -webkit-box-flex: 3;
  -webkit-flex: 3;
          flex: 3;
  border: 1rpx solid #ccc;
  width: 500rpx;
  height: 40rpx;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
}
.login_registered {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-right: 126rpx;
  margin-top: 30rpx;
}
.login_registered navigator text {
  display: block;
  border: 1rpx solid #ccc;
  width: 150rpx;
  background-color: #F8F8F8;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
  padding: 10rpx;
  text-align: center;
  font-size: 30rpx;
  font-weight: 1000;
  color: #3B4144;
}

